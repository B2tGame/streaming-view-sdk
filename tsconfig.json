{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "outDir": "./build/",
    "inlineSourceMap": false,
    "sourceMap": true,
    "sourceRoot": "./build/src",
    "strictNullChecks": true,
    "skipLibCheck": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["es2016", "dom"],
    "target": "es5",
    "allowJs": true
  },
  "include": ["src"]
}
