{
  "name": "@applandstream/streaming-view-sdk",
  "version": "4.0.0-rc.5",
  "description": "React component for Streaming Project to interact with the Edge Node Stream",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/B2tGame/streaming-view-sdk.git"
  },
  "scripts": {
    "depcheck": "depcheck --ignore-bin-package",
    "lint": "eslint src/",
    "test": "mocha  --timeout 10000 --exit --recursive 'src/**/*.tests.ts'",
    "build": "scripts/build.sh",
    "version": "cd .. && ./scripts/sync-build-info.sh",
    "style-write": "appland-style --browser --write src/",
    "style-check": "appland-style --browser --check src/",
    "postversion": "cd .. && ./scripts/postversion_hook.sh",
    "prepare": "husky install || exit 0",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "develop",
        "channel": "next",
        "prerelease": "rc"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ]
        }
      ],
      [
        "@semantic-release/npm",
        {
          "tarballDir": ".github",
          "pkgRoot": "build"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            ".github/*.tgz"
          ]
        }
      ]
    ]
  },
  "author": "Appland",
  "license": "ISC",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "homepage": "https://github.com/B2tGame/streaming-view-sdk#readme",
  "dependencies": {
    "@testrtc/watchrtc-sdk": "^1.36.3",
    "@types/node": "^18.11.18",
    "axios": "^0.27.2",
    "eventemitter3": "^4.0.7",
    "events": "^3.3.0",
    "google-protobuf": "^3.20.1",
    "grpc-web": "1.0.7",
    "pako": "^2.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-resize-observer": "^1.1.1",
    "sdp-transform": "^2.14.1",
    "socket.io-client": "^4.5.2",
    "ua-parser-js": "^1.0.2",
    "url-parse": "^1.5.10",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@applandstream/appland-style": "^1.0.4",
    "@semantic-release/git": "^10.0.1",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "prettier": "2.7.1",
    "proxyquire": "^2.1.3",
    "semantic-release": "^21.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
