{
  "name": "@applandstream/streaming-view-sdk",
  "version": "2.0.0",
  "description": "React component for Streaming Project to interact with the Edge Node Stream",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:appland/streaming-view-sdk.git"
  },
  "scripts": {
    "depcheck": "depcheck --ignores '@babel/runtime-corejs3'",
    "lint": "eslint index.js src/",
    "test": "mocha --timeout 10000 --require @babel/register --exit --recursive 'src/**/*.tests.js'",
    "build": "rm -rf build && babel index.js measurement.js -d build --source-maps && babel src -d build/src --copy-files --source-maps && echo '@applandstream/streaming-view-sdk' > build/README.md && cp package.json build/package.json",
    "version": "echo \"{\\\"tag\\\": \\\"$(cat package.json | jq -r '.version')\\\"}\" > src/controllers/build-info.json && git add src/controllers/build-info.json ",
    "postversion": "cp package.json build/package.json && (cp .npmrc build/.npmrc 2>/dev/null || true) && cp src/controllers/build-info.json build/src/controllers/build-info.json",
    "prettier-write": "prettier --write src/",
    "prettier-check": "prettier --check src/",
    "prepare": "husky install || exit 0"
  },
  "author": "Appland",
  "license": "ISC",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "homepage": "https://bitbucket.org/appland/streaming-view-sdk#readme",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.18.6",
    "axios": "^0.27.2",
    "eventemitter3": "^4.0.7",
    "google-protobuf": "^3.20.1",
    "grpc-web": "1.0.7",
    "pako": "^2.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-resize-observer": "^1.1.1",
    "screenfull": "^6.0.2",
    "sdp-transform": "^2.14.1",
    "socket.io-client": "^4.5.2",
    "ua-parser-js": "^1.0.2",
    "url-parse": "^1.5.10",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/register": "^7.18.6",
    "depcheck": "^1.4.3",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "prettier": "2.7.1",
    "proxyquire": "^2.1.3"
  }
}
